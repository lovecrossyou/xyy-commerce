{
  "name": "egg-commerce",
  "version": "1.0.0",
  "description": "yangran",
  "private": true,
  "dependencies": {
    "alipay-ftof": "^0.1.5",
    "alipay-mobile": "^3.0.8",
    "egg": "^2.20.2",
    "egg-alinode": "^2.0.1",
    "egg-cors": "^2.2.0",
    "egg-qiniu-upload": "^1.1.4",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^4.3.1",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "ms": "^2.1.1",
    "qr-image": "^3.2.0",
    "stream-wormhole": "^1.1.0",
    "tenpay": "^2.1.17",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.12.1",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.22.1",
    "eslint": "^5.15.3",
    "eslint-config-egg": "^7.3.1",
    "sequelize-auto": "^0.4.29",
    "sequelize-cli": "^5.4.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "EGG_SERVER_ENV=prod ENABLE_NODE_LOG=YES egg-sequelize db:migrate && egg-scripts start --daemon --title=xyy-commerce -p 8004",
    "stop": "egg-scripts stop --title=xyy-commerce",
    "dev": "egg-bin dev --port 8004",
    "package":"tar -zcvf ../release.tgz .",
    "test-local": "egg-bin test --full-trace",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "migrate:new": "egg-sequelize migration:create",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
